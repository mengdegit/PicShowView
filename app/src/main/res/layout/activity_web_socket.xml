<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.joker.picshowview.view.activity.WebSocketActivity">

    <Button
        android:id="@+id/webserver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="webserver"/>
    <Button
        android:id="@+id/webclient"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="webclient"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="142dp"
        android:layout_below="@+id/webserver"
        android:layout_marginTop="27dp" />
    <Button
        android:id="@+id/handler_timer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startHandler"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="142dp"
        android:layout_below="@+id/webclient"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/stop_handler"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="stopHandler"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="142dp"
        android:layout_below="@+id/handler_timer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/restart_machine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RestartMachine"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="142dp"
        android:layout_below="@+id/stop_handler"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
